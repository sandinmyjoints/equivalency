    "browserslist": "4.1.1",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-jest": "21.22.1",
    "eslint-plugin-prettier": "2.6.2",
    "expect": "23.6.0",
    "jest": "23.6.0",
    "karma": "3.0.0",
    "karma-browserstack-launcher": "1.4.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
